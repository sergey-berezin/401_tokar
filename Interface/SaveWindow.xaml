<Window x:Class="Interface.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="SaveWindow" Height="150" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Height="28" Margin="10,10,0,0" TextWrapping="Wrap" Text="Experiment name:" VerticalAlignment="Top" Width="113"/>
        <Button Content="Load" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Width="60" Click="Button_Click_Load"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="83,84,0,0" VerticalAlignment="Top" Width="60" IsCancel="True"/>
        <ComboBox x:Name="ExpName" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="270" Height="29"/>

    </Grid>
</Window>
